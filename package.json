  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "build:demo": "webpack --env production",
    "delete:dist": "cross-env rimraf dist",
    "lint": "eslint src/slickgrid-react --ext .ts",
    "build:amd": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/amd",
    "build:commonjs": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/commonjs",
    "build:esm": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/esm --module esnext --target es2018",
    "postbuild:esm": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/esm",
    "build:native-modules": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/native-modules",
    "prebuild": "npm-run-all delete:dist lint",
    "build": "npm-run-all build:amd build:commonjs build:esm build:native-modules",
    "postbuild": "npm-run-all copy-i18n:dist copy-asset-lib",
    "copy-asset-lib": "cross-env copyfiles --up 2 src/assets/lib/** dist",
    "copy-i18n:dist": "cross-env copyfiles --up 3 src/assets/i18n/**/*.* dist/i18n",
    "start:dev": "webpack serve --env development",
    "cypress": "cd test/cypress && node node_modules/cypress/bin/cypress open",
    "cypress:ci": "cd test/cypress && npm run cypress:ci",
    "test:ci": "npx jest --runInBand --coverage=true --ci --config ./test/jest.config.js",
    "test:jest": "npx jest --watch --detectOpenHandles --config test/jest.config.js",
    "test:jest:coverage": "npx jest --detectOpenHandles --runInBand --coverage --config test/jest.config.js",
    "update-npm-versions": "npm-check --skip-unused --update-all",
    "//debug": "npm run build && copyfiles dist/**/* c:/temp/slickgrid-react-test/node_modules/slickgrid-react",
    "preview:release": "release-it --dry-run",
    "release": "release-it"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
